# Abort script on invalid return value.
set -e

DEBUG=""
if [ $1 = "debug" ]; then
  DEBUG="-e -ddd"
  shift 1
fi

_SSHD_ ${DEBUG} -i -f _OPENSSHCONF_/sshd_config 
