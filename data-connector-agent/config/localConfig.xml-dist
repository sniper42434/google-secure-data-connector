<entity>
  <sdcServerHost>apps-secure-data-connector.google.com</sdcServerHost>
  <sdcServerPort>443</sdcServerPort>
  <domain>example.com</domain>
  <user>AnyValidDomainUsername</user>
  <password>ValidDomainUserPassword</password>
  <clientId>client1_example-com</clientId>
  <sshd>_START_SSHD_</sshd>
  <!-- NOTE: If you customize the socksServerPort you must also change 
       the permit port # in the woodstock-user/.ssh/authorized_keys --> 
  <socksServerPort>1080</socksServerPort>
  <apacheCtl>_APACHE_CTL_</apacheCtl>
  <apacheConfDir>_APACHE_CONF_DIR_</apacheConfDir>
  
  <!-- The following element is optional. It defines comma-separated list 
       of users who, in addition to the <user> defined above, can access 
       the healthz gadget and the associated feeds. --> 
  <healthzGadgetUsers> foo@example.com, bar@example.com </healthzGadgetUsers>
  
  <logProperties>
    # ***** Set root logger level to DEBUG and its only appender to A.
    log4j.rootLogger=info, A

    # ***** A is set to be a ConsoleAppender.
    log4j.appender.A=org.apache.log4j.ConsoleAppender
    # ***** A uses PatternLayout.
    log4j.appender.A.layout=org.apache.log4j.PatternLayout
    log4j.appender.A.layout.ConversionPattern=%-4r [%t] %-5p %c %x - %m%n
  </logProperties>
</entity>
